Python

def solve(A, B):
	m = len(A)
	n = len(B)

	# Declaring the array for storing the dp values
	dp = [[None]*(n+1) for i in range(m+1)]

	for i in range(m+1):
		for j in range(n+1):
			if i == 0 or j == 0:
				dp[i][j] = 0
			elif A[i-1] == B[j-1]:
				dp[i][j] = dp[i-1][j-1] + 1
			else:
				dp[i][j] = max(dp[i-1][j] , dp[i][j-1])

	return dp[m][n]

A = "abbcdgf"
B = "bbadcgf"
print(solve(A,B))
